@model IEnumerable<KrinPark.Models.ApplicationUser>

@{
    ViewBag.Title = "AssignRole";
}

<h2>AssignRole</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
      
       
        <th>
            @Html.DisplayNameFor(model => model.PhoneNumber)
        </th>
       
        
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.PhoneNumber)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
          
            @Html.ActionLink("Make Admin", "AssignRoles", new { id=item.Id,role= "Admin" }, new { @class="btn btn-primary"}) |
           
            @Html.ActionLink("Make Parking ", "AssignRoles", new { id = item.Id, role = "Parking" }, new { @class = "btn btn-success" }) 
            
        </td>
    </tr>
}

</table>
